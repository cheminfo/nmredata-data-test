{
  "name": "nmredata-data-test",
  "version": "0.3.0",
  "description": "provides zipped nmrRecord data files for testing purposes",
  "main": "lib/index.js",
  "module": "src/index.js",
  "types": "nmredata-data-test.d.ts",
  "files": [
    "lib",
    "src",
    "data/*",
    "nmredata-data-test.d.ts"
  ],
  "scripts": {
    "eslint": "eslint src",
    "eslint-fix": "npm run eslint -- --fix",
    "prepack": "rollup -c",
    "prettier": "prettier --check src",
    "prettier-write": "prettier --write src",
    "test": "npm run test-only && npm run eslint-fix && npm run prettier",
    "test-only": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cheminfo/nmredata-data-test.git"
  },
  "keywords": [
    "nmrRecord",
    "nmr",
    "nmredata",
    "dataset"
  ],
  "author": "J. Alejandro Bolanos A. <jose.bolanos@correounivalle.edu.co>; (https://github.com/jobo322)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cheminfo/nmredata-data-test/issues"
  },
  "homepage": "https://github.com/cheminfo/nmredata-data-test#readme",
  "jest": {
    "testEnvironment": "node"
  },
  "devDependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.18.6",
    "@types/jest": "^28.1.8",
    "eslint": "^8.23.0",
    "eslint-config-cheminfo": "^8.0.2",
    "jest": "^29.0.1",
    "prettier": "^2.7.1",
    "rollup": "^2.78.1"
  },
  "dependencies": {
    "filelist-utils": "^0.11.0"
  }
}
